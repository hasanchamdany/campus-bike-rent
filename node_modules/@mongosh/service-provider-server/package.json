{
  "name": "@mongosh/service-provider-server",
  "version": "1.6.0",
  "description": "MongoDB Shell Server Service Provider Package",
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "config": {
    "unsafe-perm": true
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/mongodb-js/mongosh.git"
  },
  "scripts": {
    "compile-ts": "tsc -p tsconfig.json",
    "test": "cross-env TS_NODE_PROJECT=../../config/tsconfig.test.json mocha -r \"../../scripts/import-expansions.js\" --timeout 60000 -r ts-node/register \"./src/**/*.spec.ts\"",
    "test-ci": "node ../../scripts/run-if-package-requested.js npm test",
    "prepublish": "npm run compile-ts",
    "lint": "eslint --report-unused-disable-directives \"./{src,test}/**/*.{js,ts,tsx}\"",
    "check": "npm run lint && depcheck --skip-missing=true --ignores aws4"
  },
  "license": "Apache-2.0",
  "publishConfig": {
    "access": "public"
  },
  "engines": {
    "node": ">=14.15.1"
  },
  "dependency-check": {
    "entries": [
      "src/**/*.js"
    ],
    "ignore": [
      "sinon",
      "chai",
      "uuid",
      "encoding"
    ]
  },
  "mongosh": {
    "ciRequiredOptionalDependencies": {
      "mongodb-client-encryption": [
        "darwin",
        "linux",
        "win32"
      ],
      "kerberos": [
        "darwin",
        "linux",
        "win32"
      ]
    }
  },
  "dependencies": {
    "@mongodb-js/devtools-connect": "^1.4.3",
    "@mongosh/errors": "1.6.0",
    "@mongosh/service-provider-core": "1.6.0",
    "@mongosh/types": "1.6.0",
    "@types/sinon-chai": "^3.2.3",
    "aws4": "^1.11.0",
    "kerberos": "^2.0.0",
    "mongodb": "^4.10.0",
    "mongodb-client-encryption": "^2.2.1",
    "mongodb-connection-string-url": "^2.5.3",
    "saslprep": "mongodb-js/saslprep#v1.0.4"
  },
  "optionalDependencies": {
    "kerberos": "^2.0.0",
    "mongodb-client-encryption": "^2.2.1"
  },
  "gitHead": "fbbf54a08fb315ae5fde693a98d439b9aa3285f8"
}
